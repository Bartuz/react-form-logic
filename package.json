{
  "name": "react-form-logic",
  "version": "0.0.2",
  "description": "react-form-logic",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run build:lib && npm run build:umd",
    "build:lib": "babel src --out-dir lib",
    "build:umd": "webpack src/index.js dist/react-form-logic.js --config webpack.development.js",
    "watch": "babel src --watch --out-dir lib",
    "test": "mocha --compilers js:babel/register --recursive 'test/**/*.js'"
  },
  "devDependencies": {
    "babel": "^5.8.33",
    "babel-core": "^5.8.33",
    "babel-loader": "^5.3.2",
    "chai": "^3.4.1",
    "mocha": "^2.3.4",
    "react": "^0.14.2",
    "react-addons-test-utils": "^0.14.2",
    "lodash": "^3.10.1",
    "webpack": "^1.12.2"
  }
}
